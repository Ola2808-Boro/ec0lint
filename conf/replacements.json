{
    "rules": {
        "no-arrow-condition": [
            "no-confusing-arrow",
            "no-constant-condition"
        ],
        "no-empty-class": [
            "no-empty-character-class"
        ],
        "no-empty-label": [
            "no-labels"
        ],
        "no-extra-strict": [
            "strict"
        ],
        "no-reserved-keys": [
            "quote-props"
        ],
        "no-space-before-semi": [
            "semi-spacing"
        ],
        "no-wrap-func": [
            "no-extra-parens"
        ],
        "space-in-brackets": [
            "object-curly-spacing"
        ],
        "space-unary-word-ops": [
            "space-unary-ops"
        ],
        "spaced-line-comment": [
            "spaced-comment"
        ]
    }
}